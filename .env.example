# Database connection (Neon/Vercel Postgres)
# Get this from your Neon dashboard: https://neon.tech
DATABASE_URL=postgresql://user:password@host/database?sslmode=require

# Admin password for /admin login
ADMIN_PASSWORD=your-secure-password-here

# Site password (leave empty to disable password protection)
SITE_PASSWORD=your-portfolio-password

# Resend API key for contact form emails
# Get one free at https://resend.com
RESEND_API_KEY=re_xxxxxxxxx
